/* Estilo para QListWidget */
QListWidget {
    border: 2px solid #e74c3c; /* Borde rojo */
    background-color: #f9d9d9; /* Fondo rojo claro (ajusta el color según tus preferencias) */
    color: #a94442; /* Color del texto */
    border-radius: 5px; /* Bordes menos redondeados */
    font-size: 14px; /* Tamaño de fuente */
    font-family: &quot;YourDesiredFont&quot;, sans-serif; /* Cambia &quot;YourDesiredFont&quot; a la tipografía que desees */
    font-weight: bold; /* Letras más gruesas */
}

/* Estilo para los botones dentro de QListWidget */
QListWidget QPushButton {
    background-color: #f9d9d9; /* Fondo rojo claro (ajusta el color según tus preferencias) */
    border: 2px solid #c0392b; /* Borde rojo más oscuro */
    color: #a94442; /* Color del texto */
    padding: 8px 16px; /* Espaciado interno */
    text-align: center; /* Alineación del texto */
    text-decoration: none; /* Decoración del texto */
    font-size: 14px; /* Tamaño de fuente */
    margin: 4px; /* Márgenes */
    border-radius: 5px; /* Bordes menos redondeados */
    font-family: &quot;YourDesiredFont&quot;, sans-serif; /* Cambia &quot;YourDesiredFont&quot; a la tipografía que desees */
    font-weight: bold; /* Letras más gruesas */
    transition: box-shadow 0.3s ease; /* Transición suave para la sombra al pasar el cursor */
}

/* Cambio de color al pasar el mouse a azul más oscuro y sombra */
QListWidget QPushButton:hover {
    background-color: #3498db; /* Cambiado a azul claro al pasar el cursor */
    box-shadow: 0 0 10px rgba(52, 152, 219, 0.5); /* Sombra de color azul claro al pasar el cursor */
}

/* Estilo para el elemento seleccionado en la QListWidget */
QListWidget::item:selected {
    background-color: #e74c3c; /* Fondo rojo cuando está seleccionado */
    color: white; /* Texto blanco cuando está seleccionado */
}

